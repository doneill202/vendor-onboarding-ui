{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/static/*",
      "/api/*"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}